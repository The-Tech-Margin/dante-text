text
@#! /bin/sh

# Generate a wordlist from a bunch a files.

(
for FILE
do
	case $FILE in
	*.Z)
		zcat $FILE ;;
	*)
		cat $FILE ;;
	esac
done
) | wordlist | tr A-Z a-z | sort -u
@
