text
@#! /bin/csh -f
#	fixname-changes BYU filenames to DDP standard names
#	Jonathan Altman on 5/10/88

foreach FILE ($argv)
	set NEWFILE=`echo $FILE | sed -e 's/DD[1-3][A-Z][A-Z]*//'`
	if (`echo $NEWFILE | wc -c` < 5) set NEWFILE=0$NEWFILE
	mv $FILE $NEWFILE:r.k
end
@
