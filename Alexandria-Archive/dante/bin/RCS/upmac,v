text
@#! /bin/csh -f

# upmac-shell script to change macintosh files into .e files after
# upload.
# Accepts multiple files as argument, labelling the output files
# with the filename, plus the suffix ".e".
#	Usage: "tomac file ...
# Written by Jonathan Altman

if ($#argv == 0) then
	echo Usage: $0 "file ..."
	exit 1
endif

umask 002
set RTFUP=$Dante/lib/rtfup.sed
foreach FILE ($argv)
echo DeMacifying $FILE
set OUTPUT=$FILE:r.e
sed -f $RTFUP $FILE | uniq > $OUTPUT
end
@
