@#! /bin/sh

: Make a tape file for Bob Kraft at the Packard Institute

FILENO=0
for FILE
do
	FILENO=`expr $FILENO + 1`
	echo ""
	echo ""
	echo '::::::::::::::::::::::::::::::::'
	echo File $FILENO: $FILE:
	echo '::::::::::::::::::::::::::::::::'
	head -10 $FILE
	dd if=$FILE of=/dev/rmt4 obs=4000 conv=block cbs=80 > /dev/null 2>&1
done
@
