head	1.3;
access;
symbols;
locks;
comment	@# @;


1.3
date	98.11.01.15.57.07;	author dante;	state Exp;
branches;
next	1.2;

1.2
date	86.09.18.09.16.59;	author dante;	state Exp;
branches;
next	1.1;

1.1
date	86.09.15.17.43.02;	author dante;	state Exp;
branches;
next	;


desc
@read a tape from BYU
@


1.3
log
@process a tape made at Brigham Young University
@
text
@#! /bin/csh -f

# read a file or files from a tape made at Brigham Young University
#
#	The output files will be named byuout.1001, byuout.1002, etc.

if ($#argv > 0) then
	echo Usage: $0
	exit 1
endif
if (-e byuout.1001) then
	echo File byuout.1001 already exists.
	exit 1
endif

@@ NUM = 01
while (1)
	if ($NUM < 10) then
		set FILE = 0$NUM.k
	else
		set FILE = $NUM.k
	endif
	dd if=/dev/rmt4 ibs=800  |  tr -d '\015' > $FILE
	if (-z $FILE) then
		rm -f $FILE
		break
	endif
	ls -l $FILE
	@@ NUM = $NUM + 1
end
mt off
@


1.2
log
@changed invocation to "byutape  file-count"
@
text
@d5 1
a5 6
#	Usage
#		byutape  count
#
#	where "count" is a positive integer equal to the number of
#	files on the tape.  The output files will be named
#	byuout.1001, byuout.1002, etc.
d7 2
a8 2
if ($#argv == 0) then
	echo Usage: $0  integer
d16 7
a22 5
@@ LIMIT = 1000 + $1
@@ NUM = 1001

while ($NUM <= $LIMIT)
	set FILE = byuout.$NUM
d24 4
@


1.1
log
@Initial revision
@
text
@d1 1
a1 1
#! /bin/sh
d3 8
a10 1
: read a file or files from a tape made at Brigham Young University
d12 8
a19 5
:	Usage
:		byutape  filename1  filename2  ...
:
:	for as many files as there are on the tape.  The files will be saved
:	as "filename1", etc.
d21 5
a25 2
for FILE
do
d28 2
a29 1
done
@
