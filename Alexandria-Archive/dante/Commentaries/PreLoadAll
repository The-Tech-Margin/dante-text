#!/bin/sh
#
# - Preloads all the comms. Some coms do not have preload script
#  John Wallace 4/2004
#
# ----------------------------------------------------------

ATOF="ambrosiane andreoli bennassuti benvenuto berthier bianchi boccaccio bosco buti cagliaritane campi carroll casini cassinese castelvetro chimenz costa daniello dellungo disiena fallani fiorentino fosca"

GTOO=" gabriele gelli giacalone giambullari grabher grandgent graziolo guido guiniforto hollander jacopo lana landino lombardi lombardus longfellow maramauro mattalia mestica momigliano oelsner ottimo"

PTOT=" padoan pasquini pietro1 pietro2 pietro3 pietrobono poem poletto porena portirelli provenzal rossetti ruskin sapegno scartazzini selmiano serravalle singleton steiner tasso  tommaseo torraca tozer trucchi"

VTOV=" vandelli varchi vellutello venturi vernon villani"
DATE=`date +"%m.%d.%y"`
echo $DATE
PWD=`pwd`

for C in $ATOF
do
 echo $C
 cd $PWD/$C
 # Test for preload
 preload > preload.out.$DATE
done
for C in $GTOO
do
 echo $C
 cd $PWD/$C
 preload > preload.out.$DATE
done
for C in $PTOT 
do
 echo $C
 cd $PWD/$C
 preload > preload.out.$DATE
done
for C in $VTOV 
do
 echo $C
 cd $PWD/$C
 preload > preload.out.$DATE
done
cd $PWD
