1.1
date     88.09.20.16.46.04;  author dante;  state Exp;
branches ;
next     ;


desc
@The canonical version of the form.db file for the database.  To generate
the real form.db, use the loadlang shell script.  
@


1.2
log
@Production version for the database load of May 1990.
@
text
@\~
\~	Form File for Database DANT
\~

\~
\~	IMPORTANT: When changing this form, edit only the canonical
\~		file, form.db.canon.  The working file, form.db is
\~		generated from the canonical file by the loadlang
\~		command whenever a commentary of a different language
\~		is to be loaded.
\~

              banner:	The Dartmouth Dante Project

         inputindent:	0

\~
\~	Paragraph Entries
\~

\~

           shortname: DOCN
            longname:	BRS Assigned Accession Number
           inputname:	..Document-Number:
               flags:	NO-SEARCH VALUES NO-POSITION DOUBLPOST PERIOD -
			COMPR-OFF
              verify:	MIN-CHARS=1 MAX-LINES=1 LINELEN=9 MASK=999999999

\~

           shortname: COMM
            longname:	Dante Project name for the commentary
           inputname:	..COMM:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ALTSTOP2 DOUBLPOST -
			NO-PERIOD COMPR-OFF
              verify:	ANYTHING MIN-CHARS=1 MAX-CHARS=75 MAX-LINES=1

\~

           shortname: LANG
            longname:	Language of the Commentary
           inputname:	..LANG:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ALTSTOP2 DOUBLPOST -
			NO-PERIOD COMPR-OFF
              verify:	LETTERS MIN-CHARS=1 MAX-CHARS=20 MAX-LINES=1

\~	Possible values for DTYP:
\~		D - Commentary Description
\~		T - Commentary Text keyed to poem line numbers
\~		I - Non-line-specific text (proemio, nota finale, etc.)
\~		P - Poem Text
\~		S - Special Dcoument

           shortname: DTYP
            longname:	Type of Document
           inputname:	..DTYP:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ALTSTOP2 NO-ABBREV COMPR-OFF
              verify:	LETTERS MIN-CHARS=1 MAX-CHARS=20 MAX-LINES=1

\~

           shortname: AUTH
            longname:	Commentary Author
           inputname:	..AUTH:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ALTSTOP2 DOUBLPOST -
			NO-PERIOD FORMAT COMPR-OFF
              verify:	ANYTHING MIN-CHARS=1

\~

           shortname: PUBD
            longname:	Original Publication Date (year)
           inputname:	..PUBD:
               flags:	NO-STDSTOP NO-ALTSTOP1 VALUES NO-POSITION
              verify:	NUMBERS AMOUNTS MIN-CHARS=4 MAX-LINES=1 MAX-CHARS=4

\~

           shortname: PUBL
            longname:	Original Publisher or Editor
           inputname:	..PUBL:
               flags:	ALTSTOP1 ALTSTOP2 DOUBLPOST NO-PERIOD FORMAT COMPR-OFF
              verify:	ANYTHING LINELEN=79

\~

           shortname: EDTR
            longname:	Dante Project Editor
           inputname:	..EDTR:
               flags:	DOUBLPOST PERIOD FORMAT COMPR-OFF
              verify:	ANYTHING MIN-CHARS=1 LINELEN=79

\~

           shortname: ATTR
            longname:	Technical Attributes of the Commentary
           inputname:	..ATTR:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ALTSTOP2 DOUBLPOST -
			PERIOD COMPR-OFF
              verify:	ANYTHING LINELEN=79

\~

           shortname: NOTE
            longname:	Miscellaneous Notes
           inputname:	..NOTE:
               flags:	DOUBLPOST NO-PERIOD FORMAT COMPR-OFF ABBREV
             subpars:	1000
              verify:	ANYTHING LINELEN=79

\~

           shortname: DENT
            longname:	Data Entry Information
           inputname:	..DENT:
               flags:	DOUBLPOST PERIOD COMPR-OFF
              verify:	ANYTHING LINELEN=79

\~

           shortname: LODD
            longname:	Date of Loading (Automatically supplied)
           inputname:	..LODD:
               flags:	NO-SEARCH DOUBLPOST PERIOD COMPR-OFF
              verify:	ANYTHING DATE-SIX DATE-REPLACE MIN-CHARS=6 -
			MAX-CHARS=6 MAX-LINES=1 LINELEN=6 MASK=999999

\~

           shortname: CTCA
            longname:	Cantica in the format 10-Inferno, etc.
           inputname:	..CTCA:
               flags:	NO-STDSTOP NO-ALTSTOP1 NO-ABBREV VALUES NO-POSITION -
			COMPR-OFF DOUBLPOST
              verify:	ANYTHING MIN-CHARS=6 MAX-CHARS=40 MAX-LINES=1

\~

           shortname: CNTO
            longname:	Canto Number (or "Proemio")
           inputname:	..CNTO:
               flags:	VALUES NO-POSITION DOUBLPOST PERIOD COMPR-OFF
              verify:	ANYTHING LINELEN=79

\~

           shortname: LINE
            longname:	First Line Number Reference
           inputname:	..LINE:
               flags:	NO-SEARCH VALUES NO-POSITION COMPR-OFF
              verify:	NUMBERS MAX-CHARS=10 MAX-LINES=1

\~

           shortname: ENDL
            longname:	Ending Line Number Reference
           inputname:	..ENDL:
               flags:	NO-SEARCH VALUES NO-POSITION COMPR-OFF
              verify:	NUMBERS MAX-CHARS=10 MAX-LINES=1

\~

           shortname: TEXT
            longname:	Commentary or Poem Text
           inputname:	..TEXT:
\~ Stopword options are set for 
               flags:	stop -
			DOUBLPOST NO-PERIOD FORMAT COMPR-OFF ABBREV
             subpars:	1000
             eoslist:	".  ",".~  ","?  ","?~  ","!  ","!~  "
              verify:	ANYTHING LINELEN=200

\~

           shortname: BTXT
            longname:	Non-searchable commentary text
           inputname:	..BTXT:
	       flags:	NO-SEARCH FORMAT COMPR-OFF
             subpars:	1000
             eoslist:	".  ",".~  ","?  ","?~  ","!  ","!~  "
              verify:	ANYTHING LINELEN=200

\~

           shortname: TCHR
            longname:	Count of Text Characters
           inputname:	..TCHR:
               flags:	NO-SEARCH NO-STDSTOP NO-ALTSTOP1
              verify:	NUMBERS AMOUNTS MAX-LINES=1 LINELEN=74

\~
\~	Equivalences for DANT Database
\~

           shortname: DESC
         equivalence:	COMM,LANG,AUTH,PUBD,PUBL,EDTR,NOTE,LODD

           shortname: HDR
         equivalence:	DOCN,COMM,LANG,DTYP,AUTH,PUBD,EDTR,ATTR,NOTE,-
			DENT,LODD,CTCA,CNTO,LINE,ENDL

           shortname: SHRT
         equivalence:	COMM,LANG,PUBD,CTCA,CNTO,LINE,ENDL

\~
\~	List Entries
\~

noiselist:
	"|"
	"\036"
	"~"
	"_"
	"+"
	"@@"
	"$"
	"#"
	","
	"{"
	"}"
:end

parbreak:
	"\n"
:end

